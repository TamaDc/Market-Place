<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="tamadc.marketplace.MenuCreative">

    <EditText
        android:id="@+id/editText2"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView14"
        android:layout_centerHorizontal="true"
        android:ems="10"
        android:inputType="none"
        android:text="Rumah creatipe merupakan media bagi masyarakat yang mempunyai kreatifitas tinggi untuk mengubah suatu barang mentah menjadi barang yang mempunyai nilai jual dengan hasil karya tangan mereka sendiri. Creatipe menyediakan tempat bagi mereka yang ingin memulai bisnis usaha di bidang kerajinan namun mempunyai keterbatasan waktu dan tempat. Creatipe menawarkan solusi bagi mereka yang membutuhkan media untuk memasarkan produk mereka tanpa harus mengorbankan cukup waktu dan tenaga dan tanpa mengganggu aktivitas lainnya. Dengan mengutamakan pelayanan," />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="18dp"
        android:text="Crative Mmarket"
        android:textAlignment="center"
        android:textSize="30sp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_below="@+id/editText2"
        android:layout_centerHorizontal="true"
        android:onClick="home_list"
        app:srcCompat="@drawable/home" />
</RelativeLayout>
